#include<stdio.h>
int main()
{
    int n,k,i,j;
    printf("enter number of rows:");
    scanf("%d",&n);
    for(i=0;i<=n;i++)
    {
        for(j=0;j<=n-i;j++)
        {
            for(k=0;k<=i;k++)
                {
                    printf("*");
                    printf("%d",i);
                }
             printf("\n");
        }
        printf("\n");
    }
    return(0);
} 
