// formation of matrix using 2D array.

#include<stdio.h>
void main()      
{
    int a[3][3],b[3][3], c[3][3],i,j;
    printf("enter 9 numbers for first matrix:\n");
    for(i=0;i<=2;i++)
        {
            for(j=0;j<=2;j++)
            {
                scanf("%d",&a[i][j]);
            }
            printf("\n");
        }
        printf("enter 9 numbers for second matrix:\n");
    for(i=0;i<=2;i++)
    for(j=0;j<=2;j++)
    scanf("%d",&b[i][j]);

    printf("your matrix after sum is:\n");

    for(i=0;i<=2;i++)
    {
        for(j=0;j<=2;j++)
        {
            c[i][j]=a[i][j]+b[i][j]; 
            printf("    %d    ",c[i][j]);
        }
        printf("\n");
    }  
}
