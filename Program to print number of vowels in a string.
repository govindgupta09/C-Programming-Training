// Program to print number vowels.

#include<stdio.h>
#include<string.h>
void main()
{
    char s[6]="govind";
    int i=0;
    int count = 0;
    while(s[i] = NULL)
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u')
        {
            count++;
        }
        i++;
    }
    printf("the number of vowels is :%d ",count);
}
