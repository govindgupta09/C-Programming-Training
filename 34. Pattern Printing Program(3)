// Pattern printing Program

#include<stdio.h>
void main()
{
    int n,i,j;
    printf("enter the number of rows:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=n;j>=i;j--)
        //printf("*");  
        printf("%d",j);   //and for diff. pattern also change j-->i
        printf("\n");
    }  
    return(0);
}
